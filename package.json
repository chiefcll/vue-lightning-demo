{
  "name": "vue-demo-app",
  "private": true,
  "version": "0.0.1",
  "description": "Lightning renderer for vue custom renderer",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/esm/index.js",
  "browser": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "vite --open",
    "build": "vite build --sourcemap=true",
    "build:github": "vite build --sourcemap=true --base=/vue-lightning-demo/",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview --open --port 8080",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:fix": "npm run lint:fix:prettier && npm run lint:fix:eslint",
    "lint:prettier": "prettier --check \"**/*.{ts,js,cjs,md}\"",
    "lint:fix:prettier": "prettier --write \"**/*.{ts,js,cjs,md}\"",
    "lint:eslint": "eslint .",
    "lint:fix:eslint": "eslint --fix .",
    "tsc": "tsc",
    "watch": "tsc -w",
    "prepare": "husky"
  },
  "keywords": [
    "lightning",
    "javascript",
    "canvas",
    "web",
    "tv"
  ],
  "author": "Chris Lorenzo",
  "license": "Apache-2.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "vite": "^5.2.8"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,cjs,md}": "prettier --write"
  },
  "dependencies": {
    "@lightningjs/renderer": "^0.8.2",
    "@lightningjs/vite-plugin-import-chunk-url": "^0.3.1",
    "@vue/reactivity": "^3.4.21",
    "@vue/runtime-core": "^3.4.21",
    "@vue/shared": "^3.4.21",
    "lightning-vue": "^0.0.1",
    "vue": "^3.4.21",
    "vue-router": "4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lightning-js/solid.git"
  },
  "files": [
    "dist",
    "src",
    "jsx-runtime.d.ts",
    "LICENSE",
    "NOTICE",
    "README.md"
  ]
}
